{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": false,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["vite.config.ts", "public/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
